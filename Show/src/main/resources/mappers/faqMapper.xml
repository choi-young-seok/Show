<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC " -//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="faq">

   
   <!-- 전체 레코드 조회 -->
   <select id="list" resultType="kr.co.show.community.domain.FaqVO">
       select faq_no,faq_category,faq_title,faq_text
      from show_faq
      order by faq_no asc
   </select>


</mapper>
