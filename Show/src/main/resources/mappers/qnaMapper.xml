<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC " -//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="qna">
 	<insert id="create">
		insert into show_qna (qna_no,qna_name,qna_phone,qna_title,qna_text,qna_category)
		values (show_qna_seq.nextval,#{qna_name},#{qna_phone},#{qna_title},#{qna_text},#{qna_category})
	</insert>
</mapper>
