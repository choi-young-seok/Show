html,body{
	font-family:"gulim";
	font-size:12px;
}
.shop_view_box{
	width:1200px;
}
.shop_info{
	width:1200px;
	height:30px;
	line-height:30px;
}
.shop_info > ul{
	overflow:hidden;
	border-top:1px solid #3d3d3d;
	border-bottom:1px solid #3d3d3d;
	box-sizing:border-box;
}
.shop_info > ul > li{
	float:left;
	text-align:center;
	border-right:1px solid #ededed;
	box-sizing:border-box;
}
.shop_info > ul > li:nth-child(1){
	width:350px;
}
.shop_info > ul > li:nth-child(2){
	width:200px;
}
.shop_info > ul > li:nth-child(3){
	width:400px;
}
.shop_info > ul > li:nth-child(4){
	width:150px;
}
.shop_info > ul > li:nth-child(5){
	width:100px;
	border:0;
}
.shop_view{
	width:1200px;
	height:150px;
	overflow:hidden;
	border-bottom:1px solid #3d3d3d;
	box-sizing:border-box;
}
.shop_view > div{
	float:left;
	height:150px;
	border-right:1px solid #ededed;
	box-sizing:border-box;
}
.shop_view > div:nth-child(5){
	border:0;
}
